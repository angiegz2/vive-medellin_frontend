# Variables de entorno para Vive Medellín

# URL base de la API (JSON Server en desarrollo)
VITE_API_BASE_URL=http://localhost:3001

# Nombre de la aplicación
VITE_APP_NAME=Vive Medellín

# Entorno de desarrollo
VITE_NODE_ENV=development

# Límite de tamaño para imágenes (en bytes)
VITE_MAX_IMAGE_SIZE=5000000

# Número máximo de eventos destacados
VITE_MAX_FEATURED_EVENTS=3